    {
      "description": "Positions scroll bar according to current line in BBTextObject",
      "fullName": "Position ScrollBar",
      "functionType": "Action",
      "name": "PositionScrollBar",
      "sentence": "Position ScrollBar _PARAM2_ from BBText object _PARAM1_",
      "events": [
        {
          "disabled": false,
          "folded": false,
          "type": "BuiltinCommonInstructions::Standard",
          "conditions": [],
          "actions": [
            {
              "type": {
                "inverted": false,
                "value": "ModVarScene"
              },
              "parameters": [
                "BBOpsTextObjBottomY",
                "=",
                "BBTextObject.Y() + BBTextObject.Variable(BBOpsBoxHeight) - (BBScrollBar.Height()*2)"
              ],
              "subInstructions": []
            },
            {
              "type": {
                "inverted": false,
                "value": "ModVarScene"
              },
              "parameters": [
                "BBOpsBoxSizeLines",
                "=",
                "BBTextObject.Variable(BBOpsBoxLines)"
              ],
              "subInstructions": []
            },
            {
              "type": {
                "inverted": false,
                "value": "ModVarScene"
              },
              "parameters": [
                "BBOpsArrayMaxLine",
                "=",
                "BBTextObject.VariableChildCount(BBOpsTextArray)"
              ],
              "subInstructions": []
            },
            {
              "type": {
                "inverted": false,
                "value": "ModVarScene"
              },
              "parameters": [
                "BBOpsArrayCurrentLine",
                "=",
                "BBTextObject.Variable(BBOpsScrollLine)"
              ],
              "subInstructions": []
            },
            {
              "type": {
                "inverted": false,
                "value": "ModVarScene"
              },
              "parameters": [
                "BBOpsBoxSizeComp",
                "=",
                "1/((BBTextObject.VariableChildCount(BBOpsTextArray)-BBTextObject.Variable(BBOpsBoxLines)) / BBTextObject.VariableChildCount(BBOpsTextArray))"
              ],
              "subInstructions": []
            },
            {
              "type": {
                "inverted": false,
                "value": "ModVarScene"
              },
              "parameters": [
                "BBOpsPercentStraight",
                "=",
                "(Variable(BBOpsArrayMaxLine) - Variable(BBOpsArrayCurrentLine))/Variable(BBOpsArrayMaxLine)"
              ],
              "subInstructions": []
            },
            {
              "type": {
                "inverted": false,
                "value": "ModVarScene"
              },
              "parameters": [
                "BBOpsFinalScrollPercent",
                "=",
                "Variable(BBOpsPercentStraight)*Variable(BBOpsBoxSizeComp)"
              ],
              "subInstructions": []
            },
            {
              "type": {
                "inverted": false,
                "value": "ModVarScene"
              },
              "parameters": [
                "BBOpsFinalScrollBarY",
                "=",
                "(BBScrollBar.Height() + Variable(BBOpsTextObjBottomY)) - (Variable(BBOpsFinalScrollPercent)*BBTextObject.Height())"
              ],
              "subInstructions": []
            },
            {
              "type": {
                "inverted": false,
                "value": "MettreX"
              },
              "parameters": [
                "BBScrollBar",
                "=",
                "BBTextObject.X()+BBTextObject.Variable(BBOpsBoxWidth)+3"
              ],
              "subInstructions": []
            },
            {
              "type": {
                "inverted": false,
                "value": "MettreY"
              },
              "parameters": [
                "BBScrollBar",
                "=",
                "round(Variable(BBOpsFinalScrollBarY))"
              ],
              "subInstructions": []
            }
          ],
          "events": []
        }
      ],
      "parameters": [
        {
          "codeOnly": false,
          "defaultValue": "",
          "description": "BBTextObject",
          "longDescription": "",
          "name": "BBTextObject",
          "optional": false,
          "supplementaryInformation": "BBText::BBText",
